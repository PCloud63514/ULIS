package org.ulis.domain;

import java.io.Serializable;
import java.util.List;

/**
 * @author https://www.facebook.com/groups/mybatis/
 */
public class NoticeBoard implements Serializable {
	private int noticeBoardNo;
	private int subjectNo;
	private List<String> gradeList;
	private char gradeOne;
	private char gradeTwo;
	private char gradeThree;
	private char gradeFour;
	private String title;
	private String content;
	private String writeDate;
	private String endDate;
	private String administratorNo;
	private int itemStart;
	private int itemSizePerPage;

	public NoticeBoard() {
	}

	public NoticeBoard(int noticeBoardNo, int subjectNo, List<String> gradeList, char gradeOne, char gradeTwo,
			char gradeThree, char gradeFour, String title, String content, String writeDate, String endDate,
			String administratorNo, int itemStart, int itemSizePerPage) {
		this.noticeBoardNo = noticeBoardNo;
		this.subjectNo = subjectNo;
		this.gradeList = gradeList;
		this.gradeOne = gradeOne;
		this.gradeTwo = gradeTwo;
		this.gradeThree = gradeThree;
		this.gradeFour = gradeFour;
		this.title = title;
		this.content = content;
		this.writeDate = writeDate;
		this.endDate = endDate;
		this.administratorNo = administratorNo;
		this.itemStart = itemStart;
		this.itemSizePerPage = itemSizePerPage;
	}

	public int getNoticeBoardNo() {
		return noticeBoardNo;
	}

	public void setNoticeBoardNo(int noticeBoardNo) {
		this.noticeBoardNo = noticeBoardNo;
	}

	public int getSubjectNo() {
		return subjectNo;
	}

	public void setSubjectNo(int subjectNo) {
		this.subjectNo = subjectNo;
	}

	public List<String> getGradeList() {
		return gradeList;
	}

	public void setGradeList(List<String> gradeList) {
		this.gradeList = gradeList;
	}

	public char getGradeOne() {
		return gradeOne;
	}

	public void setGradeOne(char gradeOne) {
		this.gradeOne = gradeOne;
	}

	public char getGradeTwo() {
		return gradeTwo;
	}

	public void setGradeTwo(char gradeTwo) {
		this.gradeTwo = gradeTwo;
	}

	public char getGradeThree() {
		return gradeThree;
	}

	public void setGradeThree(char gradeThree) {
		this.gradeThree = gradeThree;
	}

	public char getGradeFour() {
		return gradeFour;
	}

	public void setGradeFour(char gradeFour) {
		this.gradeFour = gradeFour;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getWriteDate() {
		return writeDate;
	}

	public void setWriteDate(String writeDate) {
		this.writeDate = writeDate;
	}

	public String getEndDate() {
		return endDate;
	}

	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	public String getAdministratorNo() {
		return administratorNo;
	}

	public void setAdministratorNo(String administratorNo) {
		this.administratorNo = administratorNo;
	}

	public int getItemStart() {
		return itemStart;
	}

	public void setItemStart(int itemStart) {
		this.itemStart = itemStart;
	}

	public int getItemSizePerPage() {
		return itemSizePerPage;
	}

	public void setItemSizePerPage(int itemSizePerPage) {
		this.itemSizePerPage = itemSizePerPage;
	}
}